;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	asim.pt. root.asim.pt (
			      7		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	Dnsdhcp.asim.pt.
Dnsdhcp	IN	A	10.14.40.2
2	IN 	PTR	Dnsdhcp.asim.pt.
5	IN	PTR	asim.pt.
6	IN	PTR	mail.asim.pt.
5	IN	PTR	webmin.asim.pt.
5       IN      PTR     webmail.asim.pt.
5       IN      PTR     prometheus.asim.pt.
5       IN      PTR     grafana.asim.pt.
5       IN      PTR     alerts.asim.pt.
5       IN      PTR     backup.asim.pt.

